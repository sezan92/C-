#include<iostream>
#include<string>
using namespace std;
struct book {
	string name;
	string author;
};

void printbook(struct book book1);
int main(){
	struct book book1;
	book1.name="Quran";
	book1.author = "Allah";
	printbook(book1);
	return 0;
}

void printbook(struct book book1){
	cout<<"Name "<<book1.name<<endl;
	cout<<"Author "<<book1.author<<endl;

return 0;}
